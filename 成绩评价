#include<stdio.h>
int main()
{
    int input=0;
    printf("请输入你的分数");
    scanf("%d",&input);
    if(90<=input && input<=100)
    printf("excellent\n");
    else if(75<=input &&  input<=89)
    printf("good\n");
    else if(60<=input &&  input<=74)
    printf("OK\n");
    else if(0<=input &&  input<=59)
    printf("poor\n");
    else
    printf("Wrong!");
    return 0;
}
